{
    "manifest_version": 3,
    "name": "SWE Job Filler",
    "description": "Extension that will auto fill SWE job application.",
    "version": "1.0",
    "icons": {
      "32": "job.png"
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "permissions": [
      "scripting"
    ]
  }